<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-ustomer-Response">
    <DisplayName>Extract Customer Response</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="customerId">
            <JSONPath>$.CustomerId</JSONPath>
        </Variable>
        <Variable name="customerName">
            <JSONPath>$.name</JSONPath>
        </Variable>
        <Variable name="userId">
            <JSONPath>$.UserId</JSONPath>
        </Variable>
        <Variable name="phone">
            <JSONPath>$.Phone</JSONPath>
        </Variable>
        <Variable name="address">
            <JSONPath>$.Address</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">response</Source>
</ExtractVariables>