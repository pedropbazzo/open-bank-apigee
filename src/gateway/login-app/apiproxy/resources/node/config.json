{
  "base_path": "/apis/internal/v1.0.1/loginapp",
  "logo_uri": "/apis/internal/v1.0.1/loginapp/images/logo.png",
    "jwtSignKey": "{{jwtSignKey}}",
  "authCookieName": "OB_AUTHSESSION",
  "authExpiryInSeconds":3600,
  "styles": {
    "header_background_color": "#FFF"
  },
  "authenticationTransaction": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/connector/v1.0.1/user/authenticate",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "errors": {
    "unidentified_user": {
      "code": "Login_Failed",
      "description": "User Login Failed"
    },
     "invalid_state": {
      "code":"Data_Invalid",
      "description": "Invalid State Value"
    }
  }
}


